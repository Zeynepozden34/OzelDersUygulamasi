@model List<TeacherListDto>
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Hoşgeldiniz</h1>
</div>
<div class="mt-4">
    <div class="row">
        <div class="col-2">
            @await Component.InvokeAsync("Branch")
        </div>
        <div class="col-10">
            <div class="row">
                @foreach (var teacherDto in Model)
                {
                    <div class="col-md-4">
                        @await Html.PartialAsync("_TeacherPartial", teacherDto)
                    </div>
                }
            </div>
     
        </div>

    </div>
</div>
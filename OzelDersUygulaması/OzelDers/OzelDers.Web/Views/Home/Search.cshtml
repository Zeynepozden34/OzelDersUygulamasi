@model List<TeacherDto>
<div class="row">
    <div class="col-3">
        @await Component.InvokeAsync("Branch")
        @await Html.PartialAsync("_Search")
    </div>
    <div class="col-9">
        @if (Model.Count == 0)
        {
            @await Html.PartialAsync("_NoTeacher")
        }
        else
        {
            <div class="row">
                @foreach (var teacherDto in Model)
                {
                    <div class="col-4">
                        @await Html.PartialAsync("_SearchTeacherPartial", teacherDto)
                    </div>

                }
            </div>
        }

    </div>
</div>

